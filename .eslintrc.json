{
  "extends": [
    "airbnb",
    "prettier",
    "plugin:jest/recommended"
  ],
  "plugins": [
    "prettier",
    "jest"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ],
    "class-methods-use-this": 0,
    "no-console": 0
  },
  "env": {
    "jest/globals": true,
    "node": true
  }
}